{
  "env": {
    "browser": true,
    "es6": true,
    "greasemonkey": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "no-console": "off",
    "no-use-before-define": "off",
    "no-restricted-globals": "off",
    "no-restricted-syntax": "off"
  }
}
