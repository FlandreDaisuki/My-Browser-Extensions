{
  "env": {
    "browser": true,
    "es6": true,
    "greasemonkey": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-console": "off",
    "no-use-before-define": "off",
    "no-restricted-globals": "off",
    "no-restricted-syntax": "off",
    "arrow-parens": ["error", "always"],
    "require-atomic-updates": "off"
  }
}
