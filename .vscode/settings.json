{
  "css.validate": false,
  "[css]": {
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    }
  },
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "cSpell.words": [
    "pagelet",
    "plurk",
    "stlint"
  ],
  "cSpell.ignoreWords": [
    "ssrb"
  ]
}
